<?xml version="1.0" encoding="ASCII"?>
<maksim:Model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:maksim="https://lamaksim.com"
    xsi:schemaLocation="https://lamaksim.com file_transfer_metamodel.ecore">
  <path_variables
      name="usb">
    <value>
      <steps
          xsi:type="maksim:ConcreteStep"
          value="&quot;F:/&quot;"/>
    </value>
  </path_variables>
  <path_variables
      name="personal">
    <value>
      <steps
          xsi:type="maksim:ConcreteStep"
          value="D:/Maksim"/>
    </value>
  </path_variables>
  <path_variables
      name="downloads">
    <value>
      <steps
          xsi:type="maksim:ConcreteStep"
          value="C:/users/maksim/Downloads"/>
    </value>
  </path_variables>
  <selection_variables
      name="cookBooks">
    <value>
      <where
          xsi:type="maksim:OnTags"
          treshold="1">
        <tags>it</tags>
      </where>
      <where
          xsi:type="maksim:OnAttributes"
          name="lang"
          value=""/>
      <from>
        <steps
            xsi:type="maksim:VariableStep"
            value="//@path_variables.0"/>
        <steps
            xsi:type="maksim:ConcreteStep"
            value="&quot;./Books&quot;"/>
      </from>
    </value>
  </selection_variables>
  <selection_variables
      name="tutorials">
    <value>
      <from/>
    </value>
  </selection_variables>
  <transformations
      xsi:type="maksim:Move"
      source="//@selection_variables.0"
      strategy="append">
    <destination>
      <steps
          xsi:type="maksim:VariableStep"
          value="//@path_variables.1"/>
      <steps
          xsi:type="maksim:ConcreteStep"
          value="Books/IT"/>
      <steps
          xsi:type="maksim:ConcreteStep"
          parametrized="true"
          value="lang"/>
      <steps
          xsi:type="maksim:ConcreteStep"
          value="Books"/>
    </destination>
  </transformations>
</maksim:Model>
